package springdi_assignment.tv_speaker.ver1;

public class TV {
	private SpeakerService leftSpeaker;
	private SpeakerService rightSpeaker;
	private SamsungTV samsungTV;
	private LGTV lgTV;
	public TV(SpeakerService leftSpeaker, SpeakerService rightSpeaker, SamsungTV samsungTV, LGTV lgTV) {
		super();
		this.leftSpeaker = leftSpeaker;
		this.rightSpeaker = rightSpeaker;
		this.samsungTV = samsungTV;
		this.lgTV = lgTV;
	}
	public TV() {
	}
	public SpeakerService getLeftSpeaker() {
		return leftSpeaker;
	}
	public void setLeftSpeaker(SpeakerService leftSpeaker) {
		this.leftSpeaker = leftSpeaker;
	}
	public SpeakerService getRightSpeaker() {
		return rightSpeaker;
	}
	public void setRightSpeaker(SpeakerService rightSpeaker) {
		this.rightSpeaker = rightSpeaker;
	}
	public SamsungTV getSamsungTV() {
		return samsungTV;
	}
	public void setSamsungTV(SamsungTV samsungTV) {
		this.samsungTV = samsungTV;
	}
	public LGTV getLgTV() {
		return lgTV;
	}
	public void setLgTV(LGTV lgTV) {
		this.lgTV = lgTV;
	}
	
	
}
