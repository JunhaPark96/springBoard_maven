package springdi_assignment.tv_speaker.ver1;

public class Speaker {
	private int volume;

	public int getVolume() {
		return volume;
	}

	public void setVolume(int volume) {
		this.volume = volume;
	}

	public Speaker(int volume) {
		super();
		this.volume = volume;
	}

	public Speaker() {
		super();
	}
	
	
}
