package springdi_assignment.tv_speaker.ver1;

public class TV {
	private Speaker leftSpeaker;
	private Speaker rightSpeaker;
	private int volume = 0;
	private boolean power;
	
	public Speaker getLeftSpeaker() {
		return leftSpeaker;
	}
	public void setLeftSpeaker(Speaker leftSpeaker) {
		this.leftSpeaker = leftSpeaker;
	}
	public Speaker getRightSpeaker() {
		return rightSpeaker;
	}
	public void setRightSpeaker(Speaker rightSpeaker) {
		this.rightSpeaker = rightSpeaker;
	}
	public int getVolume() {
		return volume;
	}
	public void setVolume(int volume) {
		this.volume = volume;
	}
	public boolean isPower() {
		return power;
	}
	public void setPower(boolean power) {
		this.power = power;
	}
	public TV() {
	}
	public TV(Speaker leftSpeaker, Speaker rightSpeaker, int volume, boolean power) {
		super();
		this.leftSpeaker = leftSpeaker;
		this.rightSpeaker = rightSpeaker;
		this.volume = volume;
		this.power = power;
	}
	@Override
	public String toString() {
		return "TV [leftSpeaker=" + leftSpeaker + ", rightSpeaker=" + rightSpeaker + ", volume=" + volume + ", power="
				+ power + "]";
	}
	
}
